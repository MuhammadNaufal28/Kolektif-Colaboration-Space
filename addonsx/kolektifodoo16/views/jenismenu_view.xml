<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <!-- kolektif.jenismenu tree view -->
    <record id="kolektif_jenismenu_view_tree" model="ir.ui.view">
        <field name="name">kolektif.jenismenu.view.tree</field>
        <field name="model">kolektif.jenismenu</field>
        <field name="arch" type="xml">
            <tree>
                <!-- Add your fields here -->
                <field name="kode"/>
                <field name="name"/>
                <field name="daftarbarang_ids"/>
            </tree>
        </field>
    </record>
    <!-- kolektif.jenismenu form view -->
    <record id="kolektif_jenismenu_view_form" model="ir.ui.view">
        <field name="name">kolektif.jenismenu.view.form</field>
        <field name="model">kolektif.jenismenu</field>
        <field name="arch" type="xml">
            <form string="">
                <sheet>
                    <group>
                        <!-- Add your fields here -->
                        <field name="kode"/>
                        <field name="name"/>
                        <field name="daftarbarang_ids"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <!-- kolektif.jenismenu action window -->
    <record id="kolektif_jenismenu_action" model="ir.actions.act_window">
        <field name="name">Daftar Jenis Menu</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">kolektif.jenismenu</field>
        <field name="view_mode">tree,form</field>
    </record>
    <!-- This Menu Item must have a parent and an action -->
    <menuitem id="kolektif_jenismenu_menu_act" 
            name="Kolektif Jenis Menu" 
            parent="kolektif_menu_menu_categ" 
            action="kolektif_jenismenu_action" 
            sequence="10"/>
</odoo>